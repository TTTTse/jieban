/* miniprogram/pages/editor/editor.wxss */
page{
 background-color: #f3f8f6;
}
.content{
  background-color: #fff;
  overflow: hidden;
  border-bottom: 1px dashed #eee;
  width: 710rpx;
  margin: 0 auto;
  border-radius: 8rpx;
}
.title{
  padding: 30rpx;
}
.last{
  border-bottom: 1px solid #dfe4e5;
}
.img{
  width: 60rpx;
  height: 60rpx;
  float: left;
  margin: 30rpx 12rpx 30rpx 30rpx;
}
.name{
  line-height: 120rpx;
  font-weight: 800;
  float: left;
}
.btn{
  margin: 50rpx;
  float: right;
  padding: 8rpx 40rpx;
  border: 1px solid #861055;
  font-size: 30rpx;
  border-radius: 10rpx;
  color: #861055;
}
image{
  width: 100%;
  height: 100%;
}
.text{
  float: right;
  font-size: 30rpx;
  line-height: 120rpx;
  margin-right: 50rpx;
}
